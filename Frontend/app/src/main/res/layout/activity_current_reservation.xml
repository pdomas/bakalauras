<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="560dp"
        android:layout_height="114dp"
        android:layout_marginStart="147dp"
        android:layout_marginTop="200dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Finish_reservation_btn">

        <ImageButton
            android:id="@+id/menu_btn5"
            android:layout_width="270px"
            android:layout_height="220px"
            android:backgroundTint="#154889"
            android:src="@drawable/ic_menu"
            app:iconGravity="textStart" />

        <ImageButton
            android:id="@+id/map_btn5"
            android:layout_width="270px"
            android:layout_height="220px"
            android:backgroundTint="#154889"
            android:src="@drawable/ic_map"
            app:iconGravity="textStart" />

        <ImageButton
            android:id="@+id/search5"
            android:layout_width="270px"
            android:layout_height="220px"
            android:backgroundTint="#154889"
            android:src="@drawable/ic_search"
            app:iconGravity="textStart" />

        <ImageButton
            android:id="@+id/settings5"
            android:layout_width="270px"
            android:layout_height="220px"
            android:backgroundTint="#154889"
            android:src="@drawable/ic_settings"
            app:iconGravity="textStart" />

    </LinearLayout>

    <TextView
        android:id="@+id/address_textview"
        android:layout_width="326dp"
        android:layout_height="70dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="sans-serif-medium"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/phone_number_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/phone_number_textview"
        android:layout_width="325dp"
        android:layout_height="47dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="sans-serif-medium"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/price_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_textview" />

    <TextView
        android:id="@+id/price_textview"
        android:layout_width="322dp"
        android:layout_height="44dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="58dp"
        android:fontFamily="sans-serif-medium"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/Finish_reservation_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_number_textview" />

    <Button
        android:id="@+id/Finish_reservation_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="195dp"
        android:text="Baigti rezervacijÄ…"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.617" />
</androidx.constraintlayout.widget.ConstraintLayout>